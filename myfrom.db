create database myform;
use myform;
CREATE TABLE users (

firstname VARCHAR(30) NOT NULL,
lastname VARCHAR(30) NOT NULL,
email VARCHAR(30) NOT NULL,
username VARCHAR(50) PRIMARY KEY,
password VARCHAR(50) NOT NULL
);

